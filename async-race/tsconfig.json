{
  "compilerOptions": {
    // "baseUrl": "./",
    "paths": {
      "!MODELS/*": ["./src/models/*"]
    },
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "module": "es2020",
    "target": "esNext",
    "allowJs": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true
  },
  "files": ["src/index.ts", "index.d.ts"]
}
